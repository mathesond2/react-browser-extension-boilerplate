{
  "name": "Extension Boilerplate",
  "description": "my nice description",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Extension Name",
    "default_popup": "index.html",
    "default_icon": "logo-md.png"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "icons": {
    "16": "logo-sm.png",
    "128": "logo-lg.png"
  },
  "host_permissions": ["*://*/*"]
}